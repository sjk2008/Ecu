<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecure.ecuredoctor">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logotype"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logotype"
        android:supportsRtl="true"
        android:theme="@style/Theme.EcureDoctor">
        <activity
            android:name=".userLoginInfo.UserNameActivity"
            android:exported="false" />
        <activity
            android:name=".splashScreen.SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.EcureDoctor.NoActionBar"/>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>